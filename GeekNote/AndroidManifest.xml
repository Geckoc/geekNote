      </activity>
      <!-- 增加事件Activity -->
      <activity android:name=".AddContactsActivity"></activity>
      <!-- 修改记事本Activity -->
      <activity android:name=".UpdateContactsActivity"></activity>
      <!-- 查询记事本Activity -->
      <activity android:name=".ContactsMessageActivity"></activity>
    <activity android:name="MyContactsActivity"></activity>
<activity android:name=".UserAddActivity"></activity>
<activity android:name=".Loginrest"></activity>
